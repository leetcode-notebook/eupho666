{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\cygwin64\\bin\\g++.exe",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${workspaceFolder}\\bin\\a.exe"
